{
  "manifest_version": 3,
  "name": "Whois Lookup",
  "description": "Quick Whois lookup for the current website.",
  "version": "1.0",
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "48": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
